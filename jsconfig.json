{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~sass/*": [
                "src/sass/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~img/*": [
                "src/assets/img/*"
            ],
            "~details/*": [
                "src/pages/details/*"
            ]
        }
    }
}